        "        'weather', 'temp', 'atemp', 'humidity', \r\n",
        "        'windspeed', 'casual', 'registered']\r\n",
        "df.drop(columns=cols, inplace=True)\r\n",
        "\r\n",
        "# feature engineering\r\n",
        "df = add_hours(df)\r\n",
        "df = add_isodate(df)\r\n",
        "df = add_month(df)\r\n",
