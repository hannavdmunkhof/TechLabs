        "\r\n",
        "# repeat these 2 lines outise function, else error\r\n",
        "df = pd.read_csv('bikesharing.csv')\r\n",
        "df.rename(columns={'count': 'target'}, inplace=True)\r\n",
        "\r\n",
        "# load and prepare data\r\n",
        "df = load_and_prepare_data()\r\n",
        "\r\n",
        "# drop columns\r\n",
